package listaVetores;

import java.util.Scanner;
public class vetores{
	Scanner sc = new Scanner(System.in);
	public void exercicio1();{
		 // Criar um Vetor com 5 n° inteiros 
		 int vetorInt[] = new int [5];//vetor vazio
		 int vetorInt[] = new int []{1,2,3,4,5};
		 //preenchendo o vetorInt o 1° 
		 System.out.println("Preencha o vetor de 5 posições");
		 vetorInt[0]= sc.nextInt();//1° posição índice 0
		 vetorInt[1]= sc.nextInt();//2° posição índice 1
		 vetorInt[2]= sc.nextInt();//3° posição índice 2
		 vetorInt[3]= sc.nextInt();//4° posição índice 3
		 vetorInt[4]= sc.nextInt();//5° posição índice 4
		 System.out.println("O valor da 1° posição índice 0 é"+vetorInt[0]);
		 System.out.println("O valor da 2° posição índice 1 é"+vetorInt[1]);
		 System.out.println("O valor da 3° posição índice 2 é"+vetorInt[2]);
		 System.out.println("O valor da 4° posição índice 3 é"+vetorInt[3]);
		 System.out.println("O valor da 5° posição índice 4 é"+vetorInt[4]);
	}
	public void exercicio2(){
		
	}
}





